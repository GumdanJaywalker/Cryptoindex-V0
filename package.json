    "test:routing": "echo 'Routing tests not implemented yet'",
    "test:security": "echo 'Security tests not implemented yet'",
    "test:all": "npm run test:setup && npm run test:simple",
    "hooats:start": "docker-compose -f docker-compose.hooats.yml up -d",
    "hooats:stop": "docker-compose -f docker-compose.hooats.yml down",
    "hooats:logs": "docker-compose -f docker-compose.hooats.yml logs -f hooats-api",
    "hooats:restart": "npm run hooats:stop && npm run hooats:start",
    "hooats:test": "API_BASE_URL=http://localhost:3000/api hardhat run scripts/test-hooats-existing.js --network hypervm-testnet",
    "hooats:dev": "node standalone-api-mock.cjs",
    "hooats:dev-ts": "ts-node --esm --project tsconfig.standalone.json standalone-api-fixed.ts",
    "hooats:dev-legacy": "node standalone-api.js"

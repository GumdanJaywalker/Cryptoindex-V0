    "redis:start": "docker-compose up -d",
    "redis:stop": "docker-compose down",
    "redis:logs": "docker-compose logs -f redis",
    "redis:cli": "docker exec -it hyperindex-redis redis-cli -a hyperindex_secure_password",
    "redis:test": "curl http://localhost:3000/api/redis/test",
    "hardhat:compile": "hardhat compile",
    "hardhat:test": "hardhat test",
    "deploy:testnet": "hardhat run scripts/deploy-amm.js --network hypervm-testnet",
